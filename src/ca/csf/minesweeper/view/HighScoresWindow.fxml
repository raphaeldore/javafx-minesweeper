<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.web.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane minHeight="-Infinity" minWidth="-Infinity" prefHeight="180.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ca.csf.minesweeper.controller.HighScoresWindowController">
   <center>
      <GridPane alignment="CENTER" BorderPane.alignment="CENTER">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Débutant">
               <padding>
                  <Insets left="15.0" />
               </padding>
            </Label>
            <Label text="Intermédaire" GridPane.rowIndex="1">
               <padding>
                  <Insets left="15.0" />
               </padding>
            </Label>
            <Label text="Expert" GridPane.rowIndex="2">
               <padding>
                  <Insets left="15.0" />
               </padding>
            </Label>
            <Label fx:id="lblTimeBeginner" text="Label" GridPane.columnIndex="1">
               <GridPane.margin>
                  <Insets />
               </GridPane.margin>
               <padding>
                  <Insets left="15.0" />
               </padding>
            </Label>
            <Label fx:id="lblTimeIntermediate" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="1">
               <padding>
                  <Insets left="15.0" />
               </padding>
            </Label>
            <Label fx:id="lblTimeExpert" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="2">
               <padding>
                  <Insets left="15.0" />
               </padding>
            </Label>
            <Label fx:id="lblNameBeginner" text="Label" GridPane.columnIndex="2"></Label>
            <Label fx:id="lblNameIntermediate" text="Label" GridPane.columnIndex="2" GridPane.rowIndex="1"></Label>
            <Label fx:id="lblNameExpert" text="Label" GridPane.columnIndex="2" GridPane.rowIndex="2"></Label>
         </children>
      </GridPane>
   </center>
   <bottom>
      <HBox alignment="CENTER" prefHeight="43.0" prefWidth="350.0" BorderPane.alignment="BOTTOM_CENTER">
         <children>
            <Button alignment="CENTER" mnemonicParsing="false" text="Effacer les temps" onAction="#deleteHighScores" />
            <Button alignment="CENTER" mnemonicParsing="false" text="Test SetHighScore" onAction="#setTestHighScore" />
            <Button fx:id="btnOk" alignment="CENTER" mnemonicParsing="false" text="OK">
               <HBox.margin>
                  <Insets left="15.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </bottom>
</BorderPane>
